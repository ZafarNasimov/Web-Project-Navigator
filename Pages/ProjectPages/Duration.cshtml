@page
@model WebProjectNavigator.Pages.ProjectPages.DurationModel
@{
    ViewData["Title"] = "Duration Page";
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Duration</title>
</head>

<body>

    <div>
        <ul class="nav">
            <li class="nav-item">
                <a class="nav-link btn-primary" href="#table" id="tableLink" onclick="displayDataInTable(durationData, ['agents_chatting_duration', 'count', 'duration'])">Table</a>
            </li>
        </ul>
    </div>

    <a asp-page="/Index" type="button" class="btn btn-primary backButton">Back</a>

    <div class="styled-table">
        <table id="Table" class="hidden-table">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Agents Chatting Duration</th>
                    <th>Count</th>
                    <th>Duration</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>

    <script src="/Scripts/duration.js"></script>
</body>

</html>